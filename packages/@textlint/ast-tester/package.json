    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.7.2",
    "cross-env": "^5.1.3",
    "mocha": "^5.0.1",
    "power-assert": "^1.3.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2",
    "typewiz-node": "^0.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "email": "azuciao@gmail.com"
