{
  "name": "@textlint/ast-tester",
  "version": "2.0.4",
  "description": "Compliance tests for textlint's AST(Abstract Syntax Tree).",
  "keywords": [
    "ast",
    "suite",
    "test",
    "testing",
    "textlint"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/@textlint/ast-tester",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "src/",
    "lib/"
  ],
  "main": "lib/textlint-ast-tester.js",
  "types": "lib/ast-tester.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.js\"",
    "watch": "tsc -p . --watch"
  },
  "devDependencies": {
    "@textlint/markdown-to-ast": "^6.0.5",
    "@textlint/text-to-ast": "^3.0.5",
    "@types/mocha": "^2.2.48",
