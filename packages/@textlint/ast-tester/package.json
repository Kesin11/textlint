{
  "name": "@textlint/ast-tester",
  "version": "2.0.4",
  "description": "Compliance tests for textlint's AST(Abstract Syntax Tree).",
  "keywords": [
    "ast",
    "suite",
    "test",
    "testing",
    "textlint"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/@textlint/ast-tester",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "src/",
    "lib/"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "devDependencies": {
    "@textlint/markdown-to-ast": "^6.0.5",
    "@textlint/text-to-ast": "^3.0.5",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "cross-env": "^5.1.3",
    "mocha": "^5.0.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "ts-node-test-register": "^2.0.0",
    "typescript": "^2.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "email": "azuciao@gmail.com"
}
